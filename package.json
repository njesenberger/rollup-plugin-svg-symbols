{
  "name": "rollup-plugin-svg-symbols",
  "version": "0.0.1",
  "description": "Transform imported SVG files into reusable SVG symbols",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
	"exports": {
    ".": {
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./runtime": {
      "import": "./dist/es/runtime.mjs",
      "require": "./dist/cjs/runtime.js",
      "types": "./dist/types/runtime.d.ts"
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "main": "./cjs/index.js",
    "module": "./es/index.mjs",
    "types": "./types/index.d.ts",
    "exports": {
      ".": {
        "import": "./es/index.mjs",
        "require": "./cjs/index.js",
        "types": "./types/index.d.ts"
      },
      "./runtime": {
        "import": "./es/runtime.mjs",
        "require": "./cjs/runtime.js",
        "types": "./types/runtime.d.ts"
      }
    }
  },
  "scripts": {
		"build": "rollup -c",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/njesenberger/rollup-plugin-svg-symbols"
  },
  "keywords": [
    "rollup",
    "rollup-plugin",
    "svg",
    "symbols",
    "sprite",
    "loader",
    "icons"
  ],
  "author": "Nicolas Jesenberger",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0"
  }
}
